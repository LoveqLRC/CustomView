        </activity>
        <activity android:name=".ui.base.ClipRectActivity">
        </activity>
        <activity android:name=".ui.base.DrawTextActivity">
        </activity>
        <activity android:name=".ui.base.TextBoundsActivity">
        </activity>
        <activity android:name=".ui.view.ColorTrackActivity">
        </activity>
        <activity android:name=".ui.view.RatingBarActivity">
        </activity>
        <activity android:name=".ui.view.LetterSideBarActivity">
        </activity>
        <activity android:name=".ui.base.ImageViewMatrixActivity">
        </activity>
        <activity android:name=".ui.base.BaseViewActivity">
        </activity>
        <activity android:name=".ui.viewgroup.LoadingViewActivity">
        </activity>
        <activity android:name=".ui.view.RulerViewActivity">
        </activity>
        <activity android:name=".ui.base.CustomStatesActivity">
        </activity>
        <activity android:name=".ui.view.SemiCircleActivity">
        </activity>
    </application>

</manifest>